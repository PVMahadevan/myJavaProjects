import java.util.Scanner;

public class printStar {
	
	public static void main(String[] args) {
	
		String pattern = "*";
		Scanner input = new Scanner(System.in);
		System.out.print("Enter the number:");
		int i = input.nextInt();
		
		
		for ( i = 0; i <= i; i++) {
			
			System.out.printf("%d", i);
			i++;
			
		}
		
		
	}

}
