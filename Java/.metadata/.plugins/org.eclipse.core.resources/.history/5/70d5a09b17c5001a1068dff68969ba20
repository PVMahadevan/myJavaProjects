import java.util.Scanner;

public class printStar {
	
	public static void main(String[] args) {
	
		String pattern = "*";
		Scanner input = new Scanner(System.in);
		System.out.print("Enter the number:");
		int k = input.nextInt();
		
		
		for ( i = 0; i != k; i++) {
			
			System.out.printf("%s\n", pattern);
			
			
		}
		
		
	}

}
